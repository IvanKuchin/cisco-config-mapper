interface-mappings:
- from: GigabitEthernet1
  to: Ethernet11/11
  prepend: " no switchport"
  append: " mtu 9126"
# - from: GigabitEthernet2
#   to: Ethernet22/22
# - from: GigabitEthernet3
#   to: Ethernet33/33

remove-prefixes:
- aaa
- snmp
- ntp

remove-lines:
- ' profile "CiscoTAC-1"'

prepend: |
  no system default switchport

append: |
  username cisco pass cisco  
  username cisco priv 15
  line vty 0 15
   login local
   transport input all

