interface Vlan2080
  no ip redirects
  ip address 10.200.0.2/24
  ip ospf passive-interface
  ip router ospf 131 area 0.0.0.131
  hsrp version 2
  hsrp 2000
    preempt 
    priority 110 forwarding-threshold lower 0 upper 110
    ip 10.200.0.1

interface Vlan2120
  ip flow monitor NETFLOW input  
  no ip redirects
  ip address 10.21.20.2/24
  ip ospf passive-interface
  ip ospf passive-interface
  ip router ospf 10 area 0.0.0.131
  hsrp version 2
  hsrp 2120 
    preempt delay minimum 180 
    priority 120 forwarding-threshold lower 0 upper 120
    timers  1  3
    ip 10.21.20.1

